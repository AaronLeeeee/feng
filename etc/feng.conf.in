options {
    username "feng";
    groupname "feng";
    error-log "@feng_logdir@/error.log";
    log-level 5;
};

socket {
    port "@feng_rtsp_port@";
    sctp-streams 16;
@ENABLE_SCTP@    sctp on;
@ENABLE_IPV6@    ipv6 on;
};

vhost {
    document-root "@feng_basedir@/avroot";
    document-root "@feng_basedir@/virtuals";
    access-log "@feng_logdir@/access.log";
    # use syslog instead
    # access-log "syslog";
};
